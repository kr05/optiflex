{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/views/pacientes-view/pacientes-view.html",
    "src/views/usuarios-view/usuarios-view.html",
    "src/views/examenes-view/examenes-view.html",
    "src/views/laboratorio-view/laboratorio-view.html",
    "src/views/ventas-view/ventas-view.html",
    "src/views/error-view/error-view.html"
